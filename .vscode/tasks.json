{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "dev",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "dev"
      ],
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^.*$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "^\\s*Local:.*",
          "endsPattern": "^.*ready in.*ms.*"
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "build"
      ],
      "group": {
        "kind": "build"
      },
      "problemMatcher": "$tsc"
    },
    {
      "label": "lint",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "lint"
      ],
      "problemMatcher": "$eslint-stylish"
    }
  ]
}